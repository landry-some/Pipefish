newtype

apples = clone int using +, -
oranges = clone int using +, -

fruit = abstract apples/oranges

FruitSalad = struct(qApples apples, qOranges oranges)

vector = clone list

def

string(x fruit) :
    string(int x) + " " + string(type x)

v(x ... int) :
    vector([x])

