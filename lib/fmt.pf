import

golang "fmt"

def

errorf(format string, a ... any?) : golang {
    return fmt.Errorf(format, a...)
}

sprint(a ... any?) : golang {
    return fmt.Sprint(a...)
}

sprintf(format string, a ... any?) : golang {
    return fmt.Sprintf(format, a...)
}

sprintln(a ... any?) : golang {
    return fmt.Sprintln(a...)
}
