{
    "name": "charm-highlighter",
    "displayName": "Charm highlighter",
    "description": "A basic highlighter and autoindenter for Charm.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.65.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "charm",
            "aliases": ["Charm", "charm"],
            "extensions": [".ch",".charm"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "charm",
            "scopeName": "source.charm",
            "path": "./syntaxes/charm.tmLanguage.json"
        }]
    }
}