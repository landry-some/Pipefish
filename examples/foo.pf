newtype

Color = enum RED, ORANGE, YELLOW, GREEN, BLUE, PURPLE

def

zort(x) :
    from L = [] for k::v = range x :
        L + [k::v]

evens(x) :
    from L = [] for k::v = range x :
        k % 2 == 0 :
            L + [v]
        else :
            continue

test(n) :
    from a = 0 for i = 1; i < 10; i + 1 :
        i <= n :
            a + i
        else :
            break

